<OBJECT TH-FLOOR-ETC
   (LOC GLOBAL-OBJECTS)
   (FLAGS FL-NODESC FL-SEEN FL-SURFACE FL-NEEDS-IDENTITY
          FL-HAS-DESCFCN FL-HAS-SDESC)
   (SYNONYM FLOOR CEILING SURFACE GROUND SKY CLOUD CLOUDS FOG)
   (OBJ-ADJ 0)
   (OBJ-NOUN 0)
   (ACTION RT-AC-TH-FLOOR-ETC)>

<ROUTINE RT-IDENTIFY-FLOOR? (WD "OPT" (OBJ <>)
			     "AUX" (NAM <GETP ,TH-FLOOR-ETC ,P?OBJ-NOUN>))
  <COND (<AND .OBJ <N==? .OBJ ,TH-FLOOR-ETC>>
	 <RFALSE>)>
  <COND (<EQUAL? .WD ,W?FLOOR>
	 <EQUAL? .NAM ,W?FLOOR>)
	(<EQUAL? .WD ,W?CEILING>
	 <EQUAL? .NAM ,W?CEILING>)
	(<EQUAL? .WD ,W?SKY>
	 <EQUAL? .NAM ,W?SKY ,W?CLOUD ,W?CLOUDS ,W?FOG>)
	(<EQUAL? .WD ,W?GROUND>
	 <EQUAL? .NAM ,W?SURFACE ,W?GROUND>)>>

<ROUTINE TELL-THE-FLOOR ()
  <TELL "the floor">>

<ROUTINE TELL-THE-GROUND ()
  <TELL "the ground">>

<ROUTINE RT-AC-TH-FLOOR-ETC ("OPT" (CONTEXT <>) "AUX" (INDOORS? <>) (UP? <>)
			      OBJ NXT NAM (THING ,TH-FLOOR-ETC) TOD)
  <COND (<==? .CONTEXT ,K-M-DESCFCN>
	 <COND (<AND <==? <GETP ,TH-FLOOR-ETC ,P?OBJ-NOUN> ,W?CEILING>
		     <EQUAL? ,GL-PLACE-CUR ,RM-HENRY-VII-CHAPEL>>
		<TELL ,K-BREATH-TAKING-MSG CR>
		<RTRUE>)
	       (T
		<RFALSE>)>)
	(<==? .CONTEXT ,K-M-SDESC>
	 <PRINTB <GETP .THING ,P?OBJ-NOUN>>
	 <RTRUE>)
	(<MC-T? .CONTEXT>
	 <RFALSE>)
	(<EQUAL? <SET NAM <GETP .THING ,P?OBJ-NOUN>> ,W?FLOOR
		 ,W?CEILING>
	 <SET INDOORS? T>)>
  <COND (<OR <RT-IDENTIFY-FLOOR? ,W?CEILING>
	     <RT-IDENTIFY-FLOOR? ,W?SKY>>
	 <SET UP? T>)>
  <COND (<EQUAL? .INDOORS? <MC-ISNOT? ,GL-PLACE-CUR ,FL-INDOORS>>
	 <RT-CANT-SEE-ANY-MSG .THING>
	 <RFATAL>)
	(<MC-THIS-PRSI?>
	 <COND (<MC-VERB? PUT-ON PUT THROW THROW-OVER EMPTY-INTO>
		<COND (<NOT .UP?>
		       <RT-PERFORM ,V?DROP ,GL-PRSO>)
		      (T
		       <TELL "Despite your best efforts, " THEO
			     " just won't stay up." CR>)>)>)
	(<AND <NOT .UP?> <MC-THIS-PRSO?>>
	 <COND (<MC-VERB? CROSS>
		<V-WALK-AROUND>)
	       (<MC-VERB? EXAMINE LOOK-ON SEARCH>
		<COND (<AND <FSET? .THING ,FL-HAS-DESCFCN>
			    <APPLY <GETP .THING ,P?ACTION> ,K-M-DESCFCN>>
		       <RTRUE>)>
		<RT-REMOVE-ALL ,TH-X-OBJECT>
		<SET OBJ <FIRST? ,GL-PLACE-CUR>>
		<REPEAT ()
		  <COND (.OBJ
			 <SET NXT <NEXT? .OBJ>>
			 <COND (<OR <MC-IS? .OBJ ,FL-NODESC>
				    <AND <MC-IS? .OBJ ,FL-ALIVE>
					 <MC-ISNOT? .OBJ ,FL-ASLEEP>>
				    <AND <MC-ISNOT? .OBJ ,FL-TAKEABLE>
					 <MC-ISNOT? .OBJ ,FL-TRYTAKE>>>
				<MOVE .OBJ ,TH-X-OBJECT>)>
			 <SET OBJ .NXT>)
			(T
			 <RETURN>)>>
		<COND (<FIRST? ,GL-PLACE-CUR>
		       <RT-CYOU-MSG "see" "sees">
		       <RT-PRINT-CONTENTS-2 ,GL-PLACE-CUR>)
		      (T
		       <TELL "There is nothing very "
			     <RT-PICK-NEXT ,GL-BORING-TXT>>)>
		<TELL " on " THE .THING "." CR>
		<RT-MOVE-ALL ,TH-X-OBJECT ,GL-PLACE-CUR>)>)
	(<MC-THIS-PRSO?>
	 <COND (<MC-VERB? EXAMINE LOOK-INSIDE SEARCH>
		<COND (<EQUAL? ,GL-PLACE-CUR ,RM-HENRY-VII-CHAPEL>
		       <TELL CTHE .THING " is breathtakingly beautiful." CR>)
		      (<MC-IS? ,GL-PLACE-CUR ,FL-INDOORS>
		       <TELL CTHE .THING " is high above you." CR>
		       <RTRUE>)
		      (T
		       <TELL CTHE .THING>
		       <SET TOD <RT-TIME-OF-DAY>>
		       <COND (<EQUAL? .TOD 0>
			      <TELL " is dark and foggy." CR>)
			     (<EQUAL? .TOD 1>
			      <TELL " is foggy, but at least there's enough light to see." CR>)
			     (<EQUAL? .TOD 2>
			      <TELL " is pretty light." CR>)
			     (<EQUAL? .TOD 3>
			      <TELL " is getting darker." CR>)>)>)
	       (<MC-VERB? SHOOT>
		<COND (<MC-ISNOT? ,GL-PLACE-CUR ,FL-INDOORS>
		       <TELL "The bullet disappears into the air." CR>)>)>)>>

