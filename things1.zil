; "UNION OF GLOBAL N,S,E,W WALLS, LOCAL GLOBAL N,S,E,W WALLS, AND
   GLOBAL WALLS"

<OBJECT TH-WALL
	(LOC GLOBAL-OBJECTS)
	(FLAGS FL-TRYTAKE FL-NOALL FL-NEEDS-IDENTITY FL-HAS-SDESC)
	(OBJ-ADJ 0)
	(OBJ-NOUN 0)
	(SYNONYM WALL WALLS)
	(ADJECTIVE NORTH SOUTH EAST WEST)
	(ACTION RT-AC-TH-WALL)>

<ROUTINE RT-AC-TH-WALL ("OPT" (CONTEXT <>) "AUX" (OBJ ,TH-WALL)
			(NAM <GETP .OBJ ,P?OBJ-NOUN>)
			(ADJ <GETP .OBJ ,P?OBJ-ADJ>))
  <COND (<==? .CONTEXT ,K-M-SDESC>
	 <COND (<GETP .OBJ ,P?OBJ-ADJ>
		<PRINTB .OBJ>
		<TELL " ">)>
	 <PRINTB <GETP .OBJ ,P?OBJ-NOUN>>
	 <RTRUE>)
	(<T? .CONTEXT> <RFALSE>)>
  <COND (<==? .NAM ,W?WALLS>
	 ; "Plural"
	 <COND (<MC-IS? ,GL-PLACE-CUR ,FL-INDOORS>
		<COND (<MC-VERB? SHOOT>
		       <PUTP .OBJ ,P?OBJ-NOUN ,W?WALL>
		       <PUTP .OBJ ,P?OBJ-ADJ ,W?SOUTH>
		       <RFALSE>)
		      (<MC-VERB? EXAMINE LOOK-ON>
		       <COND (<MC-HERE? ,RM-HOLMES-STUDY>
			      <TELL
			       "The walls are covered with pictures.">)>)>)
		(<MC-VERB? EXAMINE LOOK-ON>
		 <COND (<MC-HERE? ,RM-INSIDE-TRAITORS-GATE>
			<TELL "There are some ravens perched on the wall.">)>)
		(<EQUAL? ,GL-PLACE-CUR ,RM-TOTTENHAM-COURT-RD
			 ,RM-OXFORD-ST ,RM-VICTORIA-SQUARE
			 ,RM-VICTORIA-STREET ,RM-PARLIAMENT-SQUARE
			 ,RM-QUEENS-GARDENS ,RM-BUCKINGHAM-PALACE
			 ,RM-DRAWBRIDGE ,RM-BYWARD-TOWER ,RM-OUTER-WARD
			 ,RM-TOWER-GREEN ,RM-OUTSIDE-TRAITORS-GATE>
		 <RFALSE>)
		(T
		 <TELL ,K-NOT-SEE-HERE-MSG CR>)>)
	(<AND <EQUAL? .ADJ ,W?NORTH>
	      <EQUAL? ,GL-PLACE-CUR ,RM-MARYLEBONE-RD
		      ,RM-GREAT-RUSSELL-ST ,RM-THREADNEEDLE-ST
		      ,RM-MONUMENT ,RM-OUTSIDE-TRAITORS-GATE>>
	 ; "FORMER LG-NORTH-WALL"
	 <COND (<MC-VERB? EXAMINE LOOK-ON>
		<COND (<MC-HERE? ,RM-GREAT-RUSSELL-ST ,RM-THREADNEEDLE-ST
				 ,RM-MONUMENT>
		       <TELL ,K-SIGN-HERE-MSG>)>)>)
	(<AND <EQUAL? .ADJ ,W?EAST>
	      <EQUAL? ,GL-PLACE-CUR ,RM-BROAD-SANCTUARY
		      ,RM-WHITEHALL ;,RM-BLIND-ALLEY>>
	 ; "LG-EAST-WALL"
	 <COND (<MC-VERB? EXAMINE LOOK-ON>
		<COND (<MC-HERE? ,RM-BROAD-SANCTUARY ;,RM-BLIND-ALLEY>
		       <TELL ,K-SIGN-HERE-MSG>)>)>)
	(<AND <EQUAL? .ADJ ,W?WEST>
	      <EQUAL? ,GL-PLACE-CUR ,RM-221B-BAKER-ST
		      ,RM-TRAFALGAR-SQUARE ,RM-PINCHIN-LANE
		      ,RM-SWAN-LANE>>
	 ; "LG-WEST-WALL"
	 <COND (<MC-VERB? EXAMINE LOOK-ON>
		<COND (<MC-HERE? ,RM-SWAN-LANE>
		       <TELL ,K-SIGN-HERE-MSG>)>)>)
	(T
	 <COND (<F? .ADJ>
		<SET ADJ <RANDOM 4>>
		<COND (<1? .ADJ> <SET ADJ ,W?NORTH>)
		      (<==? .ADJ 2> <SET ADJ ,W?SOUTH>)
		      (<==? .ADJ 3> <SET ADJ ,W?EAST>)
		      (T <SET ADJ ,W?WEST>)>)>
	 <COND (<MC-IS? ,GL-PLACE-CUR ,FL-INDOORS>
		<COND (<==? .ADJ ,W?NORTH>
		       <COND (<MC-VERB? EXAMINE LOOK-ON>
			      <COND (<MC-HERE? ,RM-HOLMES-STUDY>
				     <TELL
"There is a large fireplace set into the north wall.">)>)>)
		      (<==? .ADJ ,W?SOUTH>
		       <COND (<MC-VERB? EXAMINE LOOK-ON>
			      <COND (<MC-HERE? ,RM-HOLMES-STUDY>
				     <TELL
"There is a patriotic \"V.R.\" spelled out in bullet marks
on the south wall." CR>)>)>)>)
	       (T
		<TELL ,K-NOT-SEE-HERE-MSG CR>)>)>>

<OBJECT TH-CROWDS
   (LOC GLOBAL-OBJECTS)
   (DESC "crowds")
   (FLAGS FL-PLURAL FL-HAS-DESCFCN)
   (SYNONYM CROWDS CROWD TOURISTS SHOPPERS WOMEN PEOPLE)
   (ACTION RT-AC-TH-CROWDS)>

<ROUTINE RT-AC-TH-CROWDS ("OPT" (CONTEXT <>))
  <COND (<OR <AND <FSET? ,GL-PLACE-CUR ,FL-INDOORS>
		  <NOT <MC-HERE? ,RM-BRITISH-MUSEUM>>>
	     <FSET? ,GL-PLACE-CUR ,FL-NO-CROWDS>>
	 <TELL "At last you've found some respite from the crowds." CR>
	 <RTRUE>)>
  <COND (<==? .CONTEXT ,K-M-DESCFCN>
	 <COND (<EQUAL? ,GL-PLACE-CUR ,RM-FLEET-STREET>
		<COND (<EQUAL? <RT-TIME-OF-DAY> 0>
		       <TELL "They're all inside, hard at work." CR>)
		      (T
		       <TELL
"They hurry past you, intent on getting to their offices." CR>)>)
	       (<EQUAL? <RT-TIME-OF-DAY> 0>
		<TELL "There are no crowds here. They're all asleep." CR>
		<RTRUE>)
	       (<EQUAL? ,GL-PLACE-CUR ,RM-TOTTENHAM-COURT-RD ,RM-OXFORD-ST>
		<TELL
"The shoppers don't appear happy that the shops are closed.">)
	       (<EQUAL? ,GL-PLACE-CUR ,RM-TRAFALGAR-SQUARE>
	        <TELL "The people are happily feeding the pigeons." CR>)
	       (<EQUAL? ,GL-PLACE-CUR ,RM-GROSVENOR-PLACE>
	        <TELL "The people are anxiously consulting their sight-seeing
maps." CR>)
	       (<EQUAL? ,GL-PLACE-CUR ,RM-THE-STRAND>
	        <COND (<EQUAL? <RT-TIME-OF-DAY> 3>
		       <TELL
"The theatre goers are happily discussing the evening's entertainment." CR>)
		      (T
		       <TELL
"The crowds are trying to select a show to attend this evening." CR>)>)
	       (T
		<COND (<EQUAL? <RT-TIME-OF-DAY> 2>
		       <TELL "They surge around you, excited about the
sight-seeing they have planned for today." CR>)
		      (<EQUAL? <RT-TIME-OF-DAY> 3>
		       <TELL "They nearly trample you in their eagerness
to get back to their hotels before darkness falls." CR>)
		      (<EQUAL? <RT-TIME-OF-DAY> 1>
		       <TELL "They surge around you, intent on their
sight-seeing." CR>)>
		<RTRUE>)>
	 <RTRUE>)
	(<EQUAL? <RT-TIME-OF-DAY> 0>
	 <TELL "There are no tourists here. They're all asleep." CR>)>>

